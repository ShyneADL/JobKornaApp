{
  "expo": {
    "name": "jobkorna",
    "slug": "jobkorna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.jobkorna.job",
    "userInterfaceStyle": "automatic",
    "image" : "latest",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jobkorna.job"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package" : "com.jobkorna.job"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      ["expo-build-properties", {
            "ios" :{
              "useFrameworks": "static"
            }
      }]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
